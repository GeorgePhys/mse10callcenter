<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>userRegistration</from-outcome>
   <to-view-id>/pages/admin/users/user.jsf</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>faqPage</from-outcome>
   <to-view-id>/pages/forum/faq.jsf</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>loginPage</from-outcome>
   <to-view-id>/pages/login/login.jsf</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>threadPage</from-outcome>
   <to-view-id>/pages/forum/thread.jsf</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/login/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>loginSuccessful</from-outcome>
   <to-view-id>/pages/home.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>createNewIssue</from-outcome>
   <to-view-id>/pages/issue/createIssue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>createNewProject</from-outcome>
   <to-view-id>/pages/issue/createProject.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>startNewSearch</from-outcome>
   <to-view-id>/pages/issue/searchIssue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>startNewProjectSearch</from-outcome>
   <to-view-id>/pages/issue/searchProject.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>reviewSoftwareProduct</from-outcome>
   <to-view-id>/pages/e_shop/softwareReview.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>reviewHardwareProduct</from-outcome>
   <to-view-id>/pages/e_shop/hardwareReview.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>AllSoftware</from-outcome>
   <to-view-id>/pages/e_shop/products.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>successRegister</from-outcome>
   <to-view-id>/pages/admin/users/completeRegistration.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>userProfile</from-outcome>
   <to-view-id>/pages/admin/users/profile.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>controlPanel</from-outcome>
   <to-view-id>/pages/admin/products/controlPanel.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>addHardware</from-outcome>
   <to-view-id>/pages/admin/products/hardware.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>addSoftware</from-outcome>
   <to-view-id>/pages/admin/products/software.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!-- navigation rules end -->
 <application>
  <locale-config>
   <default-locale>bg</default-locale>
   <supported-locale>en</supported-locale>
   <supported-locale>bg</supported-locale>
  </locale-config>
  <resource-bundle>
   <base-name>messages.messages</base-name>
   <var>messages</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>labels.labels</base-name>
   <var>labels</var>
  </resource-bundle>
 </application>
</faces-config>
