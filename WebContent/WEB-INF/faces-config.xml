<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>userDataController</managed-bean-name>
  <managed-bean-class>forms.UserDataController</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>userData</managed-bean-name>
  <managed-bean-class>forms.UserData</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginData</managed-bean-name>
  <managed-bean-class>forms.LoginData</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/userData.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>resultData</from-outcome>
   <to-view-id>/resultData.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/resultData.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>userData</from-outcome>
   <to-view-id>/userData.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>usersDataList</from-outcome>
   <to-view-id>/usersDataList.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/usersDataList.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>userData</from-outcome>
   <to-view-id>/userData.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/userLogin.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>usersDataList</from-outcome>
   <to-view-id>/usersDataList.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>UserLogin</from-outcome>
   <to-view-id>/userLogin.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <locale-config>
   <supported-locale>en</supported-locale>
   <supported-locale>bg</supported-locale>
  </locale-config>
  <message-bundle>forms.messages</message-bundle>
 </application>
</faces-config>
